<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="genweb.rectorat">
<body>

<metal:content-core fill-slot="content-core">

    <div style="clear:both;">


      <div style="width: 70%;" >
        
      </div>
    </div>



    <tal:originals tal:condition="view/isAuthenticated">
      <div tal:condition="python: 'OriginalFiles' in view.w"
           style="padding-top:20px;">
          <legend i18n:translate="">Original files</legend>
          <div tal:content="structure view/w/OriginalFiles/render" />
    </div>
    </tal:originals>

    <div tal:condition="python: 'PublishedFiles' in view.w"
         style="padding-top:20px;">
        <legend  tal:condition="view/isAuthenticated" i18n:translate="">Published files</legend>
        <div tal:content="structure view/w/PublishedFiles/render" />
    </div> 
    <br/><br/>

</metal:content-core>

</body>
</html>

