<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">
<body>

<metal:content-core fill-slot="content-core">
    <metal:content-core define-macro="content-core">

    <tal:block tal:condition="context/descripcioProposit">
      <h4> Descripció del Propòsit </h4>
    <div tal:replace="structure context/descripcioProposit/output" />
    </tal:block>

    <tal:block tal:condition="context/estatAprovacio">
      <h4> Estat Aprovació </h4>
    <div tal:replace="structure context/estatAprovacio" />
    </tal:block>

    <tal:block tal:condition="context/comentariEstatAprovacio">
      <h4> Comentari a l'estat d'aprovació </h4>
    <div tal:replace="structure context/comentariEstatAprovacio/output" />
    </tal:block>

    <tal:block tal:condition="context/fitxer">
      <h4> Fitxer </h4>
    <div tal:replace="structure context/fitxer" />
    </tal:block>

    </metal:content-core>
</metal:content-core>

</body>
</html>

